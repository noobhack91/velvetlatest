/*responsive 4/3/2023*/


@media screen and (max-width: 1199px){
    
}

@media screen and (max-width: 640px){
    .content-wrapper {
      padding: 10px;
    }
    .card-wrap {
    padding: 10px 15px 0px 15px;
}

}
@media screen and (max-width: 575px){
    
    .profile-text-dk .ck.ck-toolbar{
        width:100%;
    }
    .profile-text-dk .ck.ck-editor__editable_inline{
        width:100%;
    }
    .profile-text-dk input.title-input {
        width:100%;
    }
}


@media screen and (max-width: 480px){
    
    .my-card-tt .form-group{
        width:100%;
        padding-right:0;
    }
    .content-wrapper {
        padding:0;
    }
    .table td{
    padding: 10px 5px;
    }
    .round-small {
    width: 120px;
    height: 120px;
}
.table th {
    padding: 10px 5px;
}
.set-rates-left-table input {
    width: 100px;
}
}

