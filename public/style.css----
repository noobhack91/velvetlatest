.my-card-tt {
    display: inline-block;
    width: 100%;
}
.my-card-tt .form-group {
    display: inline-block;
    width: 49%;
    padding-right: 15px;
    vertical-align: top;
}
.my-card-tt .form-group label {
    width: 100%;
    margin: 0;
    padding-bottom: 5px;
}
.my-card-tt .form-group input, .my-card-tt .form-group select {
    height: 45px;
    line-height: 40px;
    padding: 0 10px;
    width: 100%;
    border-radius: 5px;
    border: 1px solid #fbaa52;
    outline: 0;
}
.my-card-tt .form-group textarea {
    height: 75px;
    line-height: 24px;
    padding: 0 10px;
    width: 100%;
    border-radius: 5px;
    border: 1px solid #fbaa52;
    outline: 0;
}
.my-tt-button {
    width: 100%;
    text-align: center;
}
.my-project-tt form {
    /*width: 49%;*/
    width: auto;
    width: auto;
    display: inline-block;
}

.my-minusbt {
    display: flex;
    justify-content: space-between;
    padding-bottom: 20px;
}
.my-minusbt input + input {
    margin-left: 20px;
}
.my-minusbt .remove_button {
    padding-left: 10px;
    align-self: center;
}
.upper-plus-wrap {
    width: calc(100% - 25px);
}
.upper-plus-wrap input {
    margin-bottom: 20px;
    width: 100% !important;
}
.pagination .shadow-sm.rounded-md {
    display: flex;
    justify-content: start;
}
.pagination p.leading-5 {
    display: none;
}
.pagination .shadow-sm.rounded-md svg.w-5.h-5 {
    width: 20px;
    height: 20px;
}
.pagination .flex-1{
    display: none;
}
.pagination span.relative.inline-flex.items-center.px-4.py-2.-ml-px.text-sm {
    color: #ffffff !important;
    background-color: #000000 !important;
}


